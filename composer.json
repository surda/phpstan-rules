{
    "name": "surda/phpstan-rules",
    "description": "Various PHPStan rules",
    "keywords": ["nette", "phpstan", "phpstan-rules"],
    "type": "phpstan-extension",
    "homepage": "http://surani.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Radek Šuráni",
            "homepage": "http://surani.cz",
            "email": "radek@surani.cz"
        }
    ],
    "support": {
        "email": "radek@surani.cz",
        "issues": "https://github.com/surda/phpstan-rules/issues"
    },
    "require": {
        "php": ">=8.0",
        "phpstan/phpstan": "^2.1"
    },
    "require-dev": {
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^9.6.22",
        "tracy/tracy": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Surda\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Surda\\PHPStan\\": "tests/"
        }
    },
    "scripts": {
        "da": [
            "composer dump-autoload -o"
        ]
    },
    "minimum-stability": "stable"
}
